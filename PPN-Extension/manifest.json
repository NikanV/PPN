{
  "manifest_version": 3,
  "name": "PPN",
  "description": "A simple Chrome extension to proxy traffic.",
  "version": "1.0",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "activeTab",
    "declarativeNetRequest",
    "webRequest"
  ],
  "background": {
    "service_worker": "./js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "./images/logo.png"
  },
  "icons": {
    "128": "./images/logo.png"
  }
}
  